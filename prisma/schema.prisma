datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                 String   @id @default(auto()) @map("_id") @db.ObjectId
  email              String   @unique
  firstName          String
  lastName           String
  email              String
  presence           Boolean
  plusOne            Boolean
  plusOneFirstName   String?
  plusOneLastName    String?
  dietaryRestriction String?
  message            String?
}